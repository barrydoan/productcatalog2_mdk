{
	"Controls": [
		{
			"_Type": "Control.Type.SectionedTable",
			"_Name": "SectionedTable0",
			"Sections": [
				{
					"Visible": true,
					"EmptySection": {
						"FooterVisible": false
					},
					"_Type": "Section.Type.FormCell",
					"_Name": "SectionFormCell0",
					"Controls": [
						{
							"_Type": "Control.Type.FormCell.ListPicker",
							"_Name": "FormCellListPicker0",
							"IsEditable": true,
							"IsVisible": true,
							"AllowMultipleSelection": false,
							"AllowEmptySelection": false,
							"Caption": "Choose cart",
							"DataPaging": {
								"ShowLoadingIndicator": false,
								"PageSize": 50
							},
							"PickerPrompt": "Please select one single item",
							"OnValueChange": "/ProductCatalog2/Rules/Cart/SelectDefaultCart.js",
							"IsSelectedSectionEnabled": false,
							"IsPickerDismissedOnSelection": false,
							"IsSearchCancelledAfterSelection": false,
							"AllowDefaultValueIfOneItem": false,
							"PickerItems": {
								"Target": {
									"Service": "/ProductCatalog2/Services/productcatalog2.service",
									"EntitySet": "Carts"
								},
								"DisplayValue": "{CardNo}",
								"ReturnValue": "{ID}"
							}
						}
					]
				}
			]
		}
	],
	"_Type": "Page",
	"_Name": "CartSelection",
	"Caption": "Cart Selection",
	"PrefersLargeCaption": true
